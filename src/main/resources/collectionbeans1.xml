<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="cust1" class="com.learn.mavendemo.model.Customer">
		<property name="id" value="1234"></property>
		<property name="name" value="Akash"></property>
		<property name="products">
			<map>
				<entry key="Pen" value="150"></entry>
				<entry key="Pencil" value="120"></entry>
				<entry key="Crayon" value="180"></entry>
			</map>
		</property>
	</bean>





</beans>